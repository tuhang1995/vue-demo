.main-warp
  position relative
  width 100%
  height 100%
  overflow hidden
.child-warp
  width 100%
  height 100%
  position absolute
.animationModel
  position absolute
  top 0
  right 0
  left 0
  bottom 0
  z-index 10
  background #fff
// 修改input默认填充的颜色
input
  background-color rgba(255, 255, 255, 0) !important
input:-webkit-autofill
  transition background-color 5000s ease-in-out 0s
input:-webkit-autofill:focus
  -webkit-text-fill-color #eee !important
.el-menu-vertical-demo
  text-align left
  width 240px
  min-height 400px
.el-menu-item.is-active
  color #75b9e6
  border-left 3px solid #00BEAC
  background-color #283847
.el-menu-item, .el-submenu__title
  border-left 3px solid transparent
.el-menu
  background-color #2F4051
  color #768399
.el-menu-item, .el-submenu__title
  color #768399
.el-menu-item:focus, .el-menu-item:hover, .el-submenu__title:hover
  color #75b9e6
  border-left 3px solid #00BEAC
  background-color #283847
.el-menu--collapse
  width 64px
.el-menu
  border-right none
